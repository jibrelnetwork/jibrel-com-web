{
  "connection": "default",
  "collectionName": "primaryofferings",
  "info": {
    "name": "primaryoffering",
    "description": "Primary offering"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "active": {
      "type": "boolean"
    },
    "title": {
      "type": "string"
    },
    "excerpt": {
      "type": "text"
    },
    "lang": {
      "enum": [
        "en",
        "ar"
      ],
      "type": "enumeration",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "slug": {
      "type": "string",
      "minLength": 1,
      "required": true
    },
    "pitch": {
      "type": "text"
    },
    "preview_bg_img": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "preview_bg_color": {
      "default": "#000000",
      "minLength": 7,
      "maxLength": 7,
      "type": "string"
    },
    "logo_img": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "logo_bg": {
      "default": "#ffffff",
      "minLength": 7,
      "maxLength": 7,
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "path": {
      "unique": true,
      "type": "string"
    },
    "hero_img": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "investment_goal_usd": {
      "type": "integer",
      "min": 1,
      "required": true
    },
    "investment_commited_usd": {
      "type": "integer",
      "default": "0",
      "required": true
    },
    "investment_participants": {
      "type": "integer",
      "required": true,
      "default": "0"
    },
    "investment_deadline": {
      "type": "date"
    },
    "legal_name": {
      "type": "string"
    },
    "founded": {
      "type": "string"
    },
    "employees": {
      "type": "integer",
      "min": 1
    },
    "website_url": {
      "type": "string"
    },
    "deal_valuation_usd": {
      "type": "decimal"
    },
    "deal_min_investment_usd": {
      "type": "decimal"
    },
    "deal_equity_percent": {
      "type": "decimal"
    },
    "deal_round": {
      "type": "string"
    },
    "deal_security_type": {
      "type": "string"
    },
    "tags_csv": {
      "type": "string"
    },
    "video_url": {
      "type": "string"
    },
    "social": {
      "type": "json"
    },
    "location_img": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    }
  }
}